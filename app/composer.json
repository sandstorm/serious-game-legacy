{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "sandstorm/project",
    "type": "project",
    "description": "An application by sandstorm",
    "keywords": ["laravel", "framework"],
    "license": "proprietary",
    "repositories": {
        "DistributionPackages": {
            "type": "path",
            "url": "./DistributionPackages/*"
        }
    },
    "require": {
        "php": "^8.2",
        "archilex/filament-filter-sets": "@dev",
        "awcodes/filament-tiptap-editor": "^3.4.22",
        "dutchcodingcompany/filament-developer-logins": "^1.5",
        "filament/filament": "^3.2",
        "jeffgreco13/filament-breezy": "^2.4",
        "laravel/framework": "^11.31",
        "laravel/horizon": "^5.30",
        "laravel/pulse": "^1.3",
        "laravel/reverb": "^1.0",
        "laravel/tinker": "^2.9",
        "neos/eventstore": "^1.0",
        "ralphjsmit/laravel-filament-record-finder": "@dev",
        "ralphjsmit/packages": "dev-main as 1.4.2",
        "sandstorm/eventstore-laraveladapter": "@dev"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.4",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "laravel/telescope": "^5.2",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-watch": "^3.0",
        "phpstan/phpstan": "^2.0.4",
        "phpstan/phpstan-deprecation-rules": "^2.0.1",
        "phpstan/phpstan-strict-rules": "^2.0.1",
        "spatie/laravel-ignition": "^2.9",
        "spaze/phpstan-disallowed-calls": "^4.0",
        "staabm/phpstan-todo-by": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.3.10"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "ralphjsmit/packages": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
