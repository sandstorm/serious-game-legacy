prototype(Sandstorm.ComponentLibrary:Content.LogoBar) < prototype(Neos.Neos:ContentComponent) {
    renderer = Sandstorm.ComponentLibrary:Component.LogoBar {
        amountOfLogos = ${q(node).property('amountOfLogos')}
        autoplayInterval = ${q(node).property('autoplayInterval')}

        logos = Neos.Neos:ContentCollection {
            tagName = 'div'
            nodePath = 'main'
            attributes.class = 'swiper-wrapper slider__slides'
        }
    }
}
