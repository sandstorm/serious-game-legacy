{
    "name": "ralphjsmit/laravel-filament-record-finder",
    "description": "A premium Record-finder for Filament.",
    "keywords": [
        "Ralph J. Smit",
        "laravel",
        "laravel-filament-record-finder"
    ],
    "homepage": "https://github.com/ralphjsmit/laravel-filament-record-finder",
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph J. Smit",
            "email": "rjs@ralphjsmit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "anourvalar/eloquent-serialize": "^1.2",
        "filament/forms": "^3.2.130",
        "illuminate/contracts": "^10.0|^11.0",
        "ralphjsmit/packages": "^1.4",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^7.8|^8.0",
        "orchestra/testbench": "^8.8|^9.0",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "RalphJSmit\\Filament\\RecordFinder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RalphJSmit\\Filament\\RecordFinder\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-filament-record-finder --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "ralphjsmit/packages": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "RalphJSmit\\Filament\\RecordFinder\\FilamentRecordFinderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.ralphjsmit.com"
        }
    ]
}
