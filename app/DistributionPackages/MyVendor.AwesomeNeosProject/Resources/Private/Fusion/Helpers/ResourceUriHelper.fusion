prototype(MyVendor.AwesomeNeosProject:Helper.ResourceUri) < prototype(Neos.Fusion:Component) {
    resource = null

    renderer = Neos.Fusion:Case {
        noResourceSet {
            condition = ${!props.resource}
            renderer = ''
        }
        default {
            condition = true
            renderer = Neos.Fusion:ResourceUri {
                resource = ${props.resource}
            }
        }
    }
}
